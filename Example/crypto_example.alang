// std.crypto example
import std.crypto.*;

println("UUID:");
println(randomUUID());

println("Random bytes (10):");
let bytes = getRandomValues(10);
println(bytes);

// Hash stubs (will throw for now)
try {
  println(md5("hello"));
} catch (e) {
  println(e.message);
}
try {
  println(sha1("hello"));
} catch (e) {
  println(e.message);
}
try {
  println(sha256("hello"));
} catch (e) {
  println(e.message);
}
