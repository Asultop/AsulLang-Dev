import std.encoding.*;

let s = "Hello World";
let b64 = base64.encode(s);
println("Base64 encode: " + b64);
println("Base64 decode: " + base64.decode(b64));

let hexx = hex.encode(s);
println("Hex encode: " + hexx);
println("Hex decode: " + hex.decode(hexx));

let urll = "Hello World!";
let encodedUrl = url.encode(urll);
println("URL encode: " + encodedUrl);
println("URL decode: " + url.decode(encodedUrl));